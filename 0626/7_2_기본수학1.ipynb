      "source": [
        "def solution():\n",
        "    A, B, V = map(int,input().split())\n",
        "\n",
        "    day = (V-B)/(A-B)\n",
        "    if day==int(day) : \n",
        "        day = int(day)\n",
        "    else :\n",
        "        day = int(day)+1 \n",
        "\n",
        "    print(int(day))\n",
        "    \n",
        "\n",
        "\n",
        "\n",
        "solution()"
      ],
